{
    "output": "HDMI-A-1",
    "layer": "top",
    // "position": "bottom",
    "height": 24,
    // "width": 1280,
    "spacing": 4,
    // Order of the modules
    "modules-left": ["wlr/workspaces", "custom/sep", "hyprland/window"],
    "modules-center": [],
    "modules-right": ["pulseaudio", "custom/sep", "cpu", "custom/sep", "memory", "custom/sep", "clock", "custom/sep", "tray"],
    // Modules configuration
    // "wlr/workspaces": {
    //     "disable-scroll": true,
    //     "all-outputs": true,
    //     "format": "{name}: {icon}",
    //     "format-icons": {
    //         "1": "",
    //         "2": "",
    //         "3": "",
    //         "4": "",
    //         "5": "",
    //         "urgent": "",
    //         "focused": "",
    //         "default": ""
    //     }
    // },
    "tray": {
        "spacing": 10
    },
    "clock": {
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format": " {:%a %d %b, %H:%M}",
        /* "format": "<span color=\"#89b4fa\"></span> {:%a %d %b, %H:%M}", */
    },
    "cpu": {
        "format": " {usage}%",
        "tooltip": true,
        "interval": 1,
    },
    "memory": {
        "format": " {used:0.2f}G",
    },
    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-bluetooth": "{volume}% {icon} {format_source}",
        "format-bluetooth-muted": " {icon} {format_source}",
        "format-muted": " muted",
        "format-source": " {volume}%",
        "format-source-muted": "",
        "format-icons": {
            "headphone": "",
            "hands-free": "",
            "headset": "",
            "phone": "",
            "portable": "",
            "car": "",
            "default": ["", "", ""]
        },
        "on-click": "pavucontrol"
    },
    "custom/logo": {
		"format": "",
		"tooltip": false
	},
    "custom/sep": {
		"format": "|",
		"tooltip": false
	},
}
